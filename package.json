{
  "name": "@postalsys/bounce-classifier",
  "version": "1.2.1",
  "description": "SMTP bounce message classifier using machine learning",
  "author": "Andris Reinman",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./src/index.js",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js",
      "require": "./dist/index.cjs"
    },
    "./model/*": "./model/*"
  },
  "types": "./src/index.d.ts",
  "files": [
    "src",
    "dist",
    "model",
    "!model/keras_model.h5"
  ],
  "scripts": {
    "build": "node build.js",
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "test": "node --test test/test.js"
  },
  "devDependencies": {
    "esbuild": "^0.24.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/postalsys/bounce-classifier.git"
  },
  "keywords": [
    "smtp",
    "bounce",
    "email",
    "classifier",
    "machine-learning"
  ],
  "homepage": "https://github.com/postalsys/bounce-classifier#readme",
  "bugs": {
    "url": "https://github.com/postalsys/bounce-classifier/issues"
  }
}
